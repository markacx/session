<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="342.0" prefWidth="578.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.ocie.javaone2015.build.purefx.PureFXGetSSHInfoController">
	<stylesheets>
		<URL value="@sshinfo.css" />
	</stylesheets>
	<children>
		<VBox prefHeight="338.0" prefWidth="600.0">
			<children>
				<Label text="SSH INFO">
					<font>
						<Font size="22.0" />
					</font>
					<VBox.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</VBox.margin>
				</Label>
				<Separator prefWidth="200.0" />
				<GridPane prefHeight="205.0" prefWidth="495.0" VBox.vgrow="NEVER">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" maxWidth="483.0" minWidth="0.0" prefWidth="143.0" />
						<ColumnConstraints hgrow="ALWAYS" maxWidth="600.0" minWidth="10.0" prefWidth="457.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<Label text="Host" GridPane.rowIndex="1" />
						<TextField fx:id="hostField" prefHeight="25.0" prefWidth="219.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
						<Label text="Port" GridPane.rowIndex="2" />
						<Label text="Key File" GridPane.rowIndex="3" />
						<TextField fx:id="portField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
						<HBox prefHeight="44.0" prefWidth="331.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
							<children>
								<TextField fx:id="keyFileTextField" prefHeight="25.0" prefWidth="447.0" />
								<Button mnemonicParsing="false" text="..." />
							</children>
						</HBox>
						<Label text="Pass Phrase" GridPane.rowIndex="4" />
						<Label text="Reenter Pass Phrase" GridPane.rowIndex="5" />
						<PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
						<PasswordField fx:id="reenterField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
						<Label text="User" />
						<TextField fx:id="userField" GridPane.columnIndex="1" />
					</children>
					<VBox.margin>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</VBox.margin>
				</GridPane>
				<HBox alignment="CENTER_RIGHT" prefHeight="58.0" prefWidth="600.0">
					<children>
						<Button fx:id="okButton" mnemonicParsing="false" text="Ok">
							<HBox.margin>
								<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
							</HBox.margin>
						</Button>
						<Button mnemonicParsing="false" text="Cancel">
							<HBox.margin>
								<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
							</HBox.margin>
						</Button>
					</children>
					<opaqueInsets>
						<Insets />
					</opaqueInsets>
					<VBox.margin>
						<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
					</VBox.margin>
				</HBox>
			</children>
		</VBox>
	</children>
</AnchorPane>
